{
  "apiVersion": "v1",
  "kind": "PersistentVolume",
  "metadata": {
    "name": "redis-primary-volume"
  },
  "spec": {
    "accessModes": [
      "ReadWriteOnce"
    ],
    "capacity": {
      "storage": "5Gi"
    },
    "gcePersistentDisk": {
      "fsType": "ext4",
      "pdName": "redis-primary"
    },
    "persistentVolumeReclaimPolicy": "Retain"
  }
}
